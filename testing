-- Scripts:

local function QZAAT_fake_script() -- NoGrav.NoGrav 
	local script = Instance.new('LocalScript', NoGrav)

	workspace.Parent.StarterGui.LinxHub.Phone.Frame.ImageLabel.NoGrav.NoGrav.Disabled = true
	
	script.Parent.Parent.NoGrav.MouseButton1Click:Connect(function()
		spawn(function()
			while true do
				game.Players.LocalPlayer.MaximumSimulationRadius = math.pow(math.huge,math.huge)*math.huge
				game.Players.LocalPlayer.SimulationRadius = math.pow(math.huge,math.huge)*math.huge
				game:GetService("RunService").Stepped:wait()
			end
		end)
		local function zeroGrav(part)
			if part:FindFirstChild("BodyForce") then return end
			local temp = Instance.new("BodyForce")
			temp.Force = part:GetMass() * Vector3.new(0,workspace.Gravity,0)
			temp.Parent = part
		end

		for i,v in ipairs(workspace:GetDescendants()) do
			if v:IsA("Part") and v.Anchored == false then
				if not (v:IsDescendantOf(game.Players.LocalPlayer.Character)) then
					zeroGrav(v)
					print "NoGrav Active"
				end
			end
		end

		workspace.DescendantAdded:Connect(function(part)
			if part:IsA("Part") and part.Anchored == false then
				if not (part:IsDescendantOf(game.Players.LocalPlayer.Character)) then
					zeroGrav(part)
				end
			end
		end)
	end)
end
coroutine.wrap(QZAAT_fake_script)()
local function BKRXU_fake_script() -- KillAll.KillAll 
	local script = Instance.new('LocalScript', KillAll)

	workspace.Parent.StarterGui.LinxHub.Phone.Frame.ImageLabel.KillAll.KillAll.Disabled = true
	
	script.Parent.Parent.KillAll.MouseButton1Click:Connect(function()
		local isTweening = false;

		local function tweenplayer(cframe)
			repeat wait() until game.Players.LocalPlayer.Character;
			isTweening = true
				and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") 
				and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
				and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health > 0
			local TweenInfo = TweenInfo.new(((cframe.p - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude)/105, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false)
			local Objective = {CFrame = cframe}
			local TweenPlayer = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo, Objective)
			TweenPlayer:Play()
			TweenPlayer.Completed:Wait(); wait(.1);
			isTweening = false;
		end

		local Players = game:GetService("Players")
		local LocalPlayer = Players.LocalPlayer

		local function GetClosestEnemyChar()
			local Character = LocalPlayer.Character
			local HumanoidRootPart = Character and Character:FindFirstChild("HumanoidRootPart")
			if not (Character or HumanoidRootPart) then return end

			local TargetDistance = math.huge
			local Target

			for i,v in ipairs(Players:GetPlayers()) do
				if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and Players[v.Name].Team ~= LocalPlayer.Team then
					local TargetHRP = v.Character.HumanoidRootPart
					local mag = (HumanoidRootPart.Position - TargetHRP.Position).magnitude
					if mag < TargetDistance then
						TargetDistance = mag
						Target = v
					end
				end
			end

			return Target.Character
		end

		local A_1
		local A_2 
		local A_3
		if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Sabre") then
			A_3 = game:GetService("Players").LocalPlayer.Character.Sabre
		elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild("Sword") then
			A_3 = game:GetService("Players").LocalPlayer.Character.Sword
		elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild("SapperAxe") then
			A_3 = game:GetService("Players").LocalPlayer.Character.SapperAxe
		end
		local A_4 = "Melee"
		local event = game:GetService("ReplicatedStorage").Requests.RequestDamage
		local players = game:GetService('Players')
		local LP = players.LocalPlayer

		local done = false
		local oldpos = LP.Character.HumanoidRootPart.Position

		LP.Character.Humanoid.Changed:Connect(function()
			if LP.Character.Humanoid.Health <= 0 then
				done = true
			end
		end)

		spawn(function()
			while game:GetService("RunService").RenderStepped:Wait() and LP.Character ~= nil and done == false do
				tweenplayer(GetClosestEnemyChar().HumanoidRootPart.CFrame) --+ Vector3.new(0,3,0))
				--LP.Character.HumanoidRootPart.Position = player.Character.HumanoidRootPart.Position + Vector3.new(0,3,0)
			end
		end)

		while game:GetService("RunService").RenderStepped:Wait() and LP.Character ~= nil and done == false do
			for _,player in pairs(players:GetPlayers()) do
				if player.Character and player.Team ~= LP.Team then
					A_1 = workspace[player.Name].Head
					A_2 = workspace[player.Name].Head.Position
					event:FireServer(A_1, A_2, A_3, A_4)
				end
			end
		end
		LP.Character.HumanoidRootPart.Position = oldpos
	end)
end
coroutine.wrap(BKRXU_fake_script)()
local function VIOP_fake_script() -- FastAnims.FastAnims 
	local script = Instance.new('LocalScript', FastAnims)

	workspace.Parent.StarterGui.LinxHub.Phone.Frame.ImageLabel.FastAnims.FastAnims.Disabled = true
	
	script.Parent.Parent.FastAnims.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://pastebin.com/raw/KJjfKDN4"))()
	end)
end
coroutine.wrap(VIOP_fake_script)()
