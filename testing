						wait(0.5)
						lplayer.Character.HumanoidRootPart.CFrame = NOW
						wait(0.6)
						tp(lplayer, game:GetService("Players")[v.Name])
						wait(0.4)
						lplayer.Character.HumanoidRootPart.CFrame = NOWPLR
